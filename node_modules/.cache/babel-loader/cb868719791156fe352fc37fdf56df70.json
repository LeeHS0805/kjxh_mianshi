"timeChangeSubmit\" v-if=\"timeChange\">\r\n      <el-button>批量修改一面</el-button>\r\n      <el-button>批量修改二面</el-button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport infoApi from \"@/api/info.js\";\r\n\r\nexport default {\r\n  name: \"info\",\r\n  data() {\r\n    return {\r\n      searchData: \"\",\r\n      tableData: [],\r\n      timeChange: false,\r\n    };\r\n  },\r\n  mounted() {\r\n    //获取uuid\r\n    let uuid = \"\";\r\n    //获取全部信息\r\n    infoApi.getAllStudents(uuid);\r\n    //返回接口例子\r\n    let res = {\r\n      status: 0,\r\n      students: [\r\n        {\r\n          studentId: 1,\r\n          name: \"ss\",\r\n          class: \"sdf\",\r\n          time1: \"s\",\r\n          time2: \"sdddfs\",\r\n          score1: 44,\r\n          score2: 66,\r\n        },\r\n        {\r\n          studentId: 551,\r\n          name: \"ss\",\r\n          class: \"sdf\",\r\n          time1: \"s\",\r\n          time2: \"sdddfs\",\r\n          score1: 123,\r\n          score2: 5,\r\n        },\r\n      ],\r\n      msg: \"\",\r\n    };\r\n    //处理返回信息\r\n    if (res.status === 0) {\r\n      this.tableData = res.students;\r\n    } else {\r\n      this.$message({\r\n        type: \"error\",\r\n        message: res.content,\r\n      });\r\n    }\r\n  },\r\n  methods: {\r\n    //切换到修改权值窗口\r\n    switchTo(name) {\r\n      this.$router.push({ name: name });\r\n    },\r\n    //查询详细信息\r\n    forDetail(row) {\r\n      this.$router.push(`/detail/${row.studentId}`);\r\n    },\r\n    //批量修改时间\r\n    test(selection) {\r\n      console.log(selection);\r\n    },\r\n  },\r\n  components: {},\r\n};\r\n</script>\r\n\r\n<style scoped lang=\"scss\">\r\n#info {\r\n  header {\r\n    margin-bottom: 30px;\r\n    display: flex;\r\n    justify-content: space-between;\r\n    flex-wrap: nowrap;\r\n    .warp1 {\r\n      display: flex;\r\n      flex-wrap: nowrap;\r\n      .el-input {\r\n        min-width: 200px;\r\n        max-width: 500px;\r\n      }\r\n    }\r\n    .warp2 {\r\n      display: flex;\r\n      flex-wrap: nowrap;\r\n    }\r\n  }\r\n  .timeChangeSubmit {\r\n    margin-top: 20px;\r\n    display: flex;\r\n    justify-content: flex-end;\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views"}]}