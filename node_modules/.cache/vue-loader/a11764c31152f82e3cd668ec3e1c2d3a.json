 <span>权值一</span\n        ><el-input\n          placeholder=\"修改权值\"\n          v-model=\"weightData.aspect1\"\n        ></el-input>\n      </div>\n      <div class=\"warp\">\n        <span>权值二</span\n        ><el-input\n          placeholder=\"修改权值\"\n          v-model=\"weightData.aspect2\"\n        ></el-input>\n      </div>\n      <div class=\"warp\">\n        <span>权值三</span\n        ><el-input\n          placeholder=\"修改权值\"\n          v-model=\"weightData.aspect3\"\n        ></el-input>\n      </div>\n      <div class=\"warp\">\n        <span>权值四</span\n        ><el-input\n          placeholder=\"修改权值\"\n          v-model=\"weightData.aspect4\"\n        ></el-input>\n      </div>\n    </div>\n\n    <div class=\"interview2\" v-else>\n      <div class=\"warp\">\n        <span>权值一</span\n        ><el-input\n          placeholder=\"修改权值\"\n          v-model=\"weightData.aspect1\"\n        ></el-input>\n      </div>\n      <div class=\"warp\">\n        <span>权值二</span\n        ><el-input\n          placeholder=\"修改权值\"\n          v-model=\"weightData.aspect2\"\n        ></el-input>\n      </div>\n    </div>\n\n    <el-button class=\"send\" @click=\"send\">发送修改</el-button>\n  </div>\n</template>\n\n<script>\nimport infoApi from \"@/api/info.js\";\n\nexport default {\n  name: \"weightChange\",\n  data() {\n    return {\n      window1: true,\n      weightData: {\n        aspect1: \"\",\n        aspect2: \"\",\n        aspect3: \"\",\n        aspect4: \"\",\n      },\n    };\n  },\n  mounted() {},\n  methods: {\n    back() {\n      this.$router.go(-1);\n    },\n    //发送修改\n    send() {\n      //生成数据\n      let data = \"\";\n      if (this.window1) {\n        data = {\n          uuid: sessionStorage.getItem(\"uuid\"),\n          interview: 1,\n          aspect1: this.weightData.aspect1,\n          aspect2: this.weightData.aspect2,\n          aspect3: this.weightData.aspect3,\n          aspect4: this.weightData.aspect4,\n        };\n      } else {\n        data = {\n          uuid: sessionStorage.getItem(\"uuid\"),\n          interview: 2,\n          aspect1: this.weightData.aspect1,\n          aspect2: this.weightData.aspect2,\n        };\n      }\n      //发送\n      infoApi.changeWeight(data, this);\n    },\n  },\n  components: {},\n};\n</script>\n\n<style scoped lang=\"scss\">\n#weightChange {\n  header {\n    display: flex;\n    justify-content: space-between;\n    p {\n      font-size: 28px;\n      font-weight: bold;\n    }\n  }\n  .el-switch {\n    width: max-content;\n    margin: 20px auto 0 auto;\n  }\n  .interview1,\n  .interview2 {\n    margin-top: 20px;\n    .warp {\n      margin: 10px auto 0 auto;\n      width: 80%;\n      max-width: 600px;\n      min-width: 300px;\n      display: flex;\n      span {\n        width: 30%;\n        text-align: center;\n        line-height: 40px;\n      }\n    }\n  }\n  .send {\n    display: block;\n    margin: 30px auto 0 auto;\n  }\n}\n</style>\n"]}]}