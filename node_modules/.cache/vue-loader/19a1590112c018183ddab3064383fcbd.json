p2\">\r\n        <el-button>导出excel</el-button>\r\n        <el-button @click=\"switchTo('weightChange')\">修改权值</el-button>\r\n        <el-button @click=\"timeChange = !timeChange\">修改时间</el-button>\r\n      </div>\r\n    </header>\r\n\r\n    <el-table :data=\"tableData\" @row-click=\"forDetail\" @selection-change=\"batchChange\">\r\n      <el-table-column type=\"selection\" v-if=\"timeChange\"></el-table-column>\r\n      <el-table-column prop=\"studentId\" label=\"学号\" align=\"center\"></el-table-column>\r\n      <el-table-column align=\"center\" prop=\"name\" label=\"姓名\"></el-table-column>\r\n      <el-table-column align=\"center\" prop=\"class\" label=\"班级\"></el-table-column>\r\n      <el-table-column align=\"center\" prop=\"time1\" label=\"一面时间\"></el-table-column>\r\n      <el-table-column align=\"center\" prop=\"score1\" label=\"一面分数\"></el-table-column>\r\n      <el-table-column align=\"center\" prop=\"time2\" label=\"二面时间\"></el-table-column>\r\n      <el-table-column align=\"center\" prop=\"score2\" label=\"二面分数\"></el-table-column>\r\n      <el-table-column label=\"面试时间修改\">\r\n        <template v-slot=\"scope\">\r\n          <div class=\"action\">\r\n            <el-button :disabled=\"!timeChange\" @click.stop=\"singalOpenChange(scope.row, 1)\">修改一面</el-button>\r\n            <el-button :disabled=\"!timeChange\" @click.stop=\"singalOpenChange(scope.row, 2)\">修改二面</el-button>\r\n          </div>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <div class=\"timeChangeSubmit\" v-if=\"timeChange\">\r\n      <el-button @click=\"mulOpenChange(1)\">批量修改一面时间</el-button>\r\n      <el-button @click=\"mulOpenChange(2)\">批量修改二面时间</el-button>\r\n    </div>\r\n\r\n    <Modal v-model=\"timeChangeWindow.show\" @on-ok=\"sendChange\" @on-cancel=\"cancelChange\">\r\n      <p>{{ changeTimes }}时间</p>\r\n      <div class=\"warp\">\r\n        <el-date-picker\r\n          v-model=\"timeChangeWindow.timeDate\"\r\n          type=\"date\"\r\n          placeholder=\"选择日期\"\r\n          value-format=\"yyyy-MM-dd\"\r\n        ></el-date-picker>\r\n        <el-time-select\r\n          v-model=\"timeChangeWindow.timeHour\"\r\n          :picker-options=\"{\r\n            start: '00:00',\r\n            step: '00:30',\r\n            end: '23:30',\r\n          }\"\r\n          placeholder=\"选择时间\"\r\n        ></el-time-select>\r\n      </div>\r\n    </Modal>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport infoApi from \"@/api/info.js\";\r\n\r\nexport default {\r\n  name: \"info\",\r\n  data() {\r\n    return {\r\n      searchData: \"\",\r\n      tableData: [],\r\n      timeChange: false,\r\n      timeChangeWindow: {\r\n        show: false,\r\n        timeDate: \"\",\r\n        timeHour: \"\",\r\n        interview: \"\",\r\n        studentId: [],\r\n      },\r\n      tmpArray: [],\r\n    };\r\n  },\r\n  mounted() {\r\n    //获取uuid\r\n    let uuid = \"\";\r\n    //获取全部信息\r\n    infoApi.getAllStudents(uuid);\r\n    //返回接口例子\r\n    let res = {\r\n      status: 0,\r\n      students: [\r\n        {\r\n          studentId: 1,\r\n          name: \"ss\",\r\n          class: \"sdf\",\r\n          time1: \"s\",\r\n          time2: \"sdddfs\",\r\n          score1: 44,\r\n          score2: 66,\r\n        },\r\n        {\r\n          studentId: 551,\r\n          name: \"ss\",\r\n          class: \"sdf\",\r\n          time1: \"s\",\r\n          time2: \"sdddfs\",\r\n          score1: 123,\r\n          score2: 5,\r\n        },\r\n      ],\r\n      msg: \"\",\r\n    };\r\n    //处理返回信息\r\n    if (res.status === 0) {\r\n      this.tableData = res.students;\r\n    } else {\r\n      this.$message({\r\n        type: \"error\",\r\n        message: res.content,\r\n      });\r\n    }\r\n  },\r\n  methods: {\r\n    //切换到修改权值窗口\r\n    switchTo(name) {\r\n      this.$router.push({ name: name });\r\n    },\r\n    //查询详细信息\r\n    forDetail(row) {\r\n      this.$router.push(`/detail/${row.studentId}`);\r\n    },\r\n    //批量修改时间\r\n    batchChange(selection) {\r\n      //清空数组\r\n      this.timeChangeWindow.studentId.length = 0;\r\n      //加入已选取的元素\r\n      selection.forEach((val) => {\r\n        this.timeChangeWindow.studentId.push(val.studentId);\r\n      });\r\n    },\r\n    //单行打开修改\r\n    singalOpenChange(row, index) {\r\n      //清空数组\r\n      this.timeChangeWindow.studentId.length = 0;\r\n      //加入选中单元素\r\n      this.timeChangeWindow.studentId.push(row.studentId);\r\n      //一面还是二面\r\n      this.timeChangeWindow.interview = index;\r\n      //显示窗口\r\n      this.timeChangeWindow.show = true;\r\n    },\r\n    //多选打开\r\n    mulOpenChange(index) {\r\n      //一面还是二面\r\n      this.timeChangeWindow.interview = index;\r\n      //显示窗口\r\n      this.timeChangeWindow.show = true;\r\n    },\r\n    //发送修改\r\n    sendChange() {\r\n      //生成数据\r\n      let data = {\r\n        uuid: \"\",\r\n        studentId: this.timeChangeWindow.studentId,\r\n        interview: this.timeChangeWindow.interview,\r\n        time:\r\n          this.timeChangeWindow.timeDate + \" \" + this.timeChangeWindow.timeHour,\r\n      };\r\n      //发送数据\r\n      infoApi.changeTime(data);\r\n    },\r\n    //取消修改\r\n    cancelChange() {\r\n      console.log(`取消修改`);\r\n    },\r\n  },\r\n  computed: {\r\n    changeTimes() {\r\n      return this.timeChangeWindow.interview === 1 ? \"一面\" : \"二面\";\r\n    },\r\n  },\r\n  components: {},\r\n};\r\n</script>\r\n\r\n<style scoped lang=\"scss\">\r\n#info {\r\n  header {\r\n    margin-bottom: 30px;\r\n    display: flex;\r\n    justify-content: space-between;\r\n    flex-wrap: nowrap;\r\n    .warp1 {\r\n      display: flex;\r\n      flex-wrap: nowrap;\r\n      .el-input {\r\n        min-width: 200px;\r\n        max-width: 500px;\r\n      }\r\n    }\r\n    .warp2 {\r\n      display: flex;\r\n      flex-wrap: nowrap;\r\n    }\r\n  }\r\n  .timeChangeSubmit {\r\n    margin-top: 20px;\r\n    display: flex;\r\n    justify-content: flex-end;\r\n  }\r\n}\r\n.action {\r\n  > :first-child {\r\n    margin-bottom: 10px;\r\n  }\r\n  button {\r\n    margin: 0;\r\n  }\r\n}\r\n.ivu-modal-content {\r\n  p {\r\n    font-size: 18px;\r\n    font-weight: bold;\r\n    margin-bottom: 20px;\r\n  }\r\n  .warp {\r\n    display: flex;\r\n    justify-content: space-between;\r\n  }\r\n}\r\n</style>\r\n"]}]}