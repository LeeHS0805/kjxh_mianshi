px;\n                    left:280px;\n                    .tag-box{\n                        flex: 1;\n                        width: 10vw;\n                        text-overflow:ellipsis;\n                        overflow: hidden;\n                        margin: auto 20px;\n                        white-space: nowrap;\n                    }\n\n                }\n                .tag-button1{\n                    display: block;\n                    float: right;\n                    right: 42vw;\n                }\n                .tag-input1{\n                    display: block;\n                    width: 100px;\n                    float: right;\n                }\n            }\n        }\n\n    }\n\n</style>\n\n<!--<div class=\"base_info\">基本信息</div>-->\n<!--<div class=\"base\">-->\n<!--    <div class=\"base_item\">-->\n<!--        <span>{{base[0]}}</span>-->\n<!--        <el-button type=\"primary\" icon=\"el-icon-edit\" circle></el-button>-->\n<!--    </div>-->\n<!--    <div class=\"base_item\">-->\n<!--        <span>{{base[1]}}</span>-->\n<!--    </div>-->\n<!--    <div class=\"base_item\">-->\n<!--        <span>{{base[2]}}</span>-->\n<!--    </div>-->\n<!--    <div class=\"base_item\">-->\n<!--        <span>{{base[3]}}</span>-->\n<!--    </div>-->\n<!--    <div class=\"base_item\">-->\n<!--        <span>{{base[4]}}</span>-->\n<!--    </div>-->\n<!--    <div class=\"base_item\">-->\n<!--        <span>{{base[5]}}</span>-->\n<!--    </div>-->\n<!--</div>-->\n"]}]}