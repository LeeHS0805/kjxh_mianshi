utton type=\"primary\" class=\"info-button\" @click=\"back\">返回上层</el-button>\n            </div>\n            <div class=\"interview-info-content\">\n                <div class=\"interview-info-content1\" v-for=\"(item,index) in base\" :key=\"index\">\n                    <div class=\"studentTag\">{{item.label}}</div>\n                    <el-button icon=\"el-icon-plus\" circle class=\"tag-button1\" @click=\"showTagStore\"></el-button>\n                    <div class=\"tag\" >\n                        <div class=\"tag-box\" v-for=\"(tags,i) in item.tag\" :key=\"i\">{{tags}}</div>\n                    </div>\n                    <el-input v-model=\"score\" placeholder=\"输入得分\" class=\"tag-input1\"></el-input>\n                </div>\n            </div>\n        </div>\n        <tagStore v-else-if=\"!isShow\"></tagStore>\n    </div>\n</template>\n\n<script>\n    import tagStore from \"./tagStore\";\n    export default {\n        name: \"interview_info\",\n        data(){\n            return{\n                isShow:true,\n                score:'',\n                base:[\n                    {\n                        label:'c语言',\n                        tag:['1','2'],\n                        score:''\n                    },\n                    {\n                        label:'计算机相关',\n                        tag:[],\n                        score:''\n                    },\n                    {\n                        label:'其他',\n                        tag:[],\n                        score:''\n                    },\n                    {\n                        label:'task',\n                        tag:[],\n                        score:''\n                    },\n                    {\n                        label:'评论',\n                        tag:[],\n                        score:''\n                    }\n                ],\n                tags: [\n                    { name: '标签一', type: '' }\n                ]\n            }\n        },\n       